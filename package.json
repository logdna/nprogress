{
  "name": "@logdna/nprogress",
  "author": "Rico Sta. Cruz <hi@ricostacruz.com>",
  "description": "Simple slim progress bars",
  "version": "0.2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/logdna/nprogress.git"
  },
  "scripts": {
    "test": "nyc mocha",
    "test:ci": "mocha test --reporter mocha-junit-reporter --reporter-options mochaFile=./coverage/test.xml",
    "release": "semantic-release"
  },
  "main": "nprogress.js",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.4",
    "jquery": "^3.6.0",
    "jsdom": "^16.6.0",
    "mocha": "^8.4.0",
    "mocha-jsdom": "^2.0.0",
    "mocha-junit-reporter": "^2.0.0",
    "nyc": "^15.1.0",
    "semantic-release": "^17.4.3",
    "semantic-release-config-logdna": "^1.3.0"
  },
  "dependencies": {},
  "jspm": {
    "format": "global",
    "shim": {
      "nprogress": {
        "deps": [
          "./nprogress.css!"
        ]
      }
    },
    "dependencies": {
      "css": "*"
    }
  },
  "spm": {
    "main": "nprogress.js",
    "output": [
      "nprogress.css"
    ],
    "ignore": [
      "support",
      "test",
      "vendor"
    ]
  },
  "release": {
    "branches": [
      "master"
    ],
    "extends": "semantic-release-config-logdna"
  }
}
